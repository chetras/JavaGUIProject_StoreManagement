<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserDashboardcontroller">
    <children>
        <Label layoutX="446.0" layoutY="8.0" prefHeight="35.0" prefWidth="286.0" text="Store Management System" textFill="#222121">
            <font>
                <Font name="System Italic" size="24.0" />
            </font>
        </Label>
        <HBox prefHeight="700.0" prefWidth="160.0" style="-fx-background-color: gray;">
            <children>
                <StackPane prefHeight="700.0" prefWidth="90.0">
                    <children>
                        <AnchorPane prefHeight="700.0" prefWidth="98.0">
                            <children>
                                <Button layoutX="21.0" layoutY="316.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="116.0" style="-fx-background-insets: 0,0,-1,0;" text="Setting">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Button>
                                <Button layoutX="21.0" layoutY="63.0" mnemonicParsing="false" onAction="#onHomebtn" prefHeight="86.0" prefWidth="116.0" style="-fx-background-insets: 0,0,-1,0;" text="Home">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Button>
                                <Button layoutX="21.0" layoutY="149.0" mnemonicParsing="false" onAction="#onOrderbtn" prefHeight="81.0" prefWidth="116.0" style="-fx-background-insets: 0,0,-1,0;" text="Order">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Button>
                                <Button layoutX="21.0" layoutY="230.0" mnemonicParsing="false" onAction="#onProductbtn" prefHeight="86.0" prefWidth="116.0" style="-fx-background-insets: 0,0,-1,0;" text="Product">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Button>
                                <Button layoutX="39.0" layoutY="537.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="80.0" style="-fx-background-color: red;" text="Log out" textFill="#fcf7f7" />
                            </children>
                        </AnchorPane>
                    </children>
                </StackPane>
            </children>
        </HBox>
        <TableView fx:id="productData" layoutX="160.0" layoutY="57.0" prefHeight="432.0" prefWidth="832.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="ProdId" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="ProdName" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="ProdPrice" prefWidth="128.0" text="Price" />
                <TableColumn fx:id="ProdCat" prefWidth="132.0" text="Catagory" />
                <TableColumn fx:id="ProdStock" prefWidth="75.0" text="Stock" />
            </columns>
        </TableView>
        <TextField fx:id="txtProdId" layoutX="220.0" layoutY="503.0" prefHeight="31.0" prefWidth="240.0" promptText="ProductID" />
        <Button layoutX="672.0" layoutY="544.0" mnemonicParsing="false" onAction="#searchOnUserClick" text="Search Product" />
        <TextField fx:id="txtProdName" layoutX="220.0" layoutY="541.0" prefHeight="31.0" prefWidth="240.0" promptText="ProductName" />
        <TextField fx:id="txtProdPrice" layoutX="220.0" layoutY="580.0" prefHeight="31.0" prefWidth="240.0" promptText="ProductPrice" />
        <TextField fx:id="txtProdCat" layoutX="220.0" layoutY="619.0" prefHeight="31.0" prefWidth="240.0" promptText="ProductCatagory" />
        <TextField fx:id="txtProdStock" layoutX="220.0" layoutY="655.0" prefHeight="31.0" prefWidth="240.0" promptText="ProductStock" />
        <Button layoutX="599.0" layoutY="596.0" mnemonicParsing="false" onAction="#displayuserTable" text="Show All Product" />
        <Label fx:id="text" layoutX="511.0" layoutY="655.0" prefHeight="21.0" prefWidth="214.0" textAlignment="CENTER" />
        <Button layoutX="524.0" layoutY="544.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="116.0" text="Buy Product" />
    </children>
</AnchorPane>
